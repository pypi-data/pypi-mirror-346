        [ sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path gocam:number_of_activities ],
        [ sh:class gocam:TermObject ;
            sh:nodeKind sh:IRI ;
