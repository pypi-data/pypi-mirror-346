description: Tests various lsmcli commands that did not fit into other category.

/capabilities_success:
  description: Tests 'lsmcli capabilities'.
  tier: 1
  test: capabilities.py

/capabilities_fail:
  description: Tests 'lsmcli capabilities' failures.
  tier: 2
  test: capabilities.py
  tags+: [smispy]

/iscsi_chap_success:
  description: Tests 'lsmcli iscsi-chap'.
  tier: 1
  test: iscsi_chap.py
  requires_setup+:
    - lsm/setup/create_volume
    - lsm/setup/create_access_group_chap

/plugin_info:
  description: Tests getting plugin info using 'lsmcli plugin-info'.
  tier: 1
  test: plugin_info.py
  tags+: [smispy]

/pool_member_info_success:
  description: Tests 'lsmcli pool-member-info'.
  tier: 1
  test: pool_member_info.py

/pool_member_info_fail:
  description: Tests 'lsmcli pool-member-info' failures.
  tier: 2
  test: pool_member_info.py

/system_read_cache_pct_update_success:
  description: Tests 'lsmcli system-read-cache-pct-update'.
  tier: 1
  test: system_read_cache_pct_update.py
  tags: [sim] # not supported on ontap

/system_read_cache_pct_update_fail:
  description: Tests 'lsmcli system-read-cache-pct-update' failures.
  tier: 2
  test: system_read_cache_pct_update.py
  tags: [sim] # not supported on ontap
