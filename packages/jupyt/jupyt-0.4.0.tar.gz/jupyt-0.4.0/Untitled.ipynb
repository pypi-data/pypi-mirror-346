   "source": [
    "def factorial(n):\n",
    "    \"\"\"Compute the factorial of a non-negative integer n.\"\"\"\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n - 1)\n",
    "\n",
    "# Example usage:\n",
    "num = 5\n",
    "fact_result = factorial(num)\n",
    "print(f\"Factorial of {num} is {fact_result}\")"
   ]
  }
